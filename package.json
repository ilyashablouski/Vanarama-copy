  "scripts": {
    "analyze-build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 ANALYZE=true yarn build",
    "apollo:generate": "rimraf generated && apollo client:codegen generated --target=typescript --outputFlat --no-addTypename --excludes=*/graphQlTests/*",
    "build": "next build",
    "build:scss": "rimraf public/styles && node-sass --source-map true src/core/styles -o public/styles --output-style compressed",
    "dev": "cross-env NODE_ENV=development node express.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "start": "NODE_ENV=production NODE_OPTIONS=--max_old_space_size=4096 node express-cluster.js",
    "start:win": "cross-env NODE_ENV=production NODE_OPTIONS=--max_old_space_size=4096 node express-cluster.js",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test": "jest",
    "test:graphql": "node src/graphqlTests/index.js",
    "pm2": "pm2 start pm2.json --env production",
    "pm2-docker": "pm2-runtime start pm2.json --env production",
    "prebuild": "npm run build:scss",
    "predev": "npm run build:scss",
    "typecheck": "tsc --noEmit",
    "storybook": "cross-env ENV=storybook yarn start-storybook -p 9001 -c .storybook"
  },
