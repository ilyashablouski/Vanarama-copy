{
    "containerDefinitions": [
        {
            "environment": [
            {
              "name": "LOCALHOST_IP",
              "value": "127.0.0.1"
            },
            {
              "name": "DOCKER_IP",
              "value": "0.0.0.0"
            },
            {
              "name": "NODE_ENV",
              "value": "development"
            },
            {
              "name": "FORCE_COLOR",
              "value": "true"
            }
            ],
            "essential": true,
            "image": "000379120260.dkr.ecr.eu-west-2.amazonaws.com/autorama-nextstorefront:latest",
            "name": "nextstorefront",
            "portMappings": [
                {
                    "containerPort": 6601,
                    "hostPort": 0
                }
            ],
            "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
              "awslogs-group": "Grid-Development",
              "awslogs-region": "eu-west-2",
              "awslogs-stream-prefix": "storefront2"
              }
            },
            "memory": 750,
            "cpu": 10
        }
    ],
    "family": ""
  }
